<div id="map"></div>

<script>
  function initMap(){

    national_stadium = {lat: 35.677824, lng: 139.714541}

    map = new google.maps.Map(document.getElementById('map'), {
      center: national_stadium,
      zoom: 12,
    });

    marker = new google.maps.Marker({
      position: national_stadium,
      map: map
    });
  }
</script>
