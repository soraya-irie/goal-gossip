<table class="table table-striped">
  <thead>
    <tr>
      <th>順位</th>
      <th>クラブ名</th>
      <th>勝ち点</th>
      <th>試合数</th>
      <th>勝</th>
      <th>分</th>
      <th>敗</th>
    </tr>
  </thead>
  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= team['position'] %></td>
        <td><%= team['name'] %></td>
        <td><%= team['points'] %></td>
        <td><%= team['matchesPlayed'] %></td>
        <td><%= team['seasonWins_overall'] %></td>
        <td><%= team['seasonDraws_overall'] %></td>
        <td><%= team['seasonLosses_overall'] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
